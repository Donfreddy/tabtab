{
  "author": "mklabs",
  "name": "tabtab",
  "description": "tab completion helpers, for node cli programs. Inspired by npm completion.",
  "main": "lib/index.js",
  "scripts": {
    "test": "DEBUG='tabtab*' c8 mocha --timeout 5000",
    "posttest": "npm run eslint",
    "mocha": "DEBUG='tabtab*' mocha --timeout 5000",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "coverage-html": "npm run mocha && c8 report --reporter=html && serve coverage",
    "eslint": "eslint lib/ test/",
    "watch": "npm-watch",
    "quick-test": "DEBUG='tabtab*' node test/fixtures/installer-install.js",
    "bash-quick-test": "DEBUG=tabtab* COMP_CWORD='2' COMP_LINE='tabtab foo' COMP_POINT='10' tabtab completion -- tabtab foo"
  },
  "watch": {
    "test": "{lib,test}/**/*.js"
  },
  "devDependencies": {
    "c8": "^3.2.0",
    "coveralls": "^3.0.2",
    "eslint-config-mklabs": "^1.0.9",
    "inquirer-test": "^2.0.1",
    "mocha": "^5.2.0",
    "npm-watch": "^0.4.0",
    "serve": "^10.0.2"
  },
  "license": "MIT",
  "keywords": [
    "terminal",
    "tab",
    "unix",
    "console",
    "complete",
    "completion"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mklabs/tabtab.git"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "inquirer": "^6.0.0",
    "minimist": "^1.2.0",
    "untildify": "^3.0.3"
  },
  "version": "3.0.0-alpha"
}
